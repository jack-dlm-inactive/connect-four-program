import java.util.Scanner;
class Main 
{
	static Scanner sc = new Scanner(System.in);
	static String name = "";
	static String nameOne = "";
	static String nameTwo = "";
	static String nameThree = "";
	static String nameFour = "";
	
	public static void main(String[] args) 
	{
		getName();
		printQueue();
		getNameOne();
		printQueue();
		getNameTwo();
		printQueue();
		getNameThree();
		printQueue();
		getNameFour();
		printQueue();
		System.out.println("The help queue is full, sorry!");
	}
	public static String getName()
	{
  		System.out.println("> state your name:");
  		name = sc.nextLine();
  		while(true)
  		{
	  		if(name.equals("Colee") || name.equals("colee"))
	  		{
	  			System.out.println("\nGo back to your desk and try writing some of your own code.");
	  			break;
	  		}
	  		else if(name.equals("Kellen") || name.equals("kellen"))
	  		{
	  			System.out.println("\nWelcome Sensei.");
	  			break;
	  		}
	  		else if(name.equals("Mason") || name.equals("mason"))
	  		{
	  			System.out.println("\nMasonnnnnnnnnnnnnn");
	  			break;
	  		}
	  		else
	  		{
	  			System.out.println("name not found in our databases. You are on the help queue.");
	  			break;
	  		}
  		}
  		return name;
	}
	public static String getNameOne()
	{
  		System.out.println("\nadd a second name to the help queue:");
  		nameOne = sc.nextLine();
  		while(true)
  		{
  			if(nameOne.equals(name))
  			{
  				System.out.println("your name is already on the queue!");
  				break;
  			}
  			else
  			{
		  		if(nameOne.equals("Colee") || nameOne.equals("colee"))
		  		{
		  			System.out.println("\nGo back to your desk and try writing some of your own code.");
		  			break;
		  		}
		  		else if(nameOne.equals("Kellen") || nameOne.equals("kellen"))
		  		{
		  			System.out.println("\nWelcome Sensei.");
		  			break;
		  		}
		  		else if(nameOne.equals("Mason") || nameOne.equals("mason"))
		  		{
		  			System.out.println("\nMasonnnnnnnnnnnnnn");
		  			break;
		  		}
		  		else
		  		{
		  			System.out.println("name not found in our databases. You are on the help queue.");
		  			break;
		  		}
  			}
  		}
  		return nameOne;
	}
	public static String getNameTwo()
	{
  		System.out.println("\nadd a third name to the help queue:");
  		nameTwo = sc.nextLine();
  		while(true)
  		{
  			if(nameTwo.equals(name) || nameTwo.equals(nameOne))
  			{
  				System.out.println("your name is already on the queue!");
  				break;
  			}
  			else
  			{
		  		if(nameTwo.equals("Colee") || nameTwo.equals("colee"))
		  		{
		  			System.out.println("\nGo back to your desk and try writing some of your own code.");
		  			break;
		  		}
		  		else if(nameTwo.equals("Kellen") || nameTwo.equals("kellen"))
		  		{
		  			System.out.println("\nWelcome Sensei.");
		  			break;
		  		}
		  		else if(nameTwo.equals("Mason") || nameTwo.equals("mason"))
		  		{
		  			System.out.println("\nMasonnnnnnnnnnnnnn");
		  			break;
		  		}
		  		else
		  		{
		  			System.out.println("name not found in our databases. You are on the help queue.");
		  			break;
		  		}
  			}
  		}
  		return nameTwo;
	}
	public static String getNameThree()
	{
  		System.out.println("\nadd a fourth name to the help queue:");
  		nameThree = sc.nextLine();
  		while(true)
  		{
  			if(nameThree.equals(name) || nameThree.equals(nameOne) || nameThree.equals(nameTwo))
  			{
  				System.out.println("your name is already on the queue!");
  				break;
  			}
	  		if(nameThree.equals("Colee") || nameThree.equals("colee"))
	  		{
	  			System.out.println("\nGo back to your desk and try writing some of your own code.");
	  			break;
	  		}
	  		else if(nameThree.equals("Kellen") || nameThree.equals("kellen"))
	  		{
	  			System.out.println("\nWelcome Sensei.");
	  			break;
	  		}
	  		else if(nameThree.equals("Mason") || nameThree.equals("mason"))
	  		{
	  			System.out.println("\nMasonnnnnnnnnnnnnn");
	  			break;
	  		}
	  		else
	  		{
	  			System.out.println("name not found in our databases. You are on the help queue.");
	  			break;
	  		}
  		}
  		return nameThree;
	}
	public static String getNameFour()
	{
  		System.out.println("\nadd a fifth name to the help queue:");
  		nameFour = sc.nextLine();
  		while(true)
  		{
  			if(nameFour.equals(name) || nameFour.equals(nameOne) || nameFour.equals(nameTwo) || nameFour.equals(nameThree))
  			{
  				System.out.println("your name is already on the queue!");
  				break;
  			}
  			else
  			{
		  		if(nameFour.equals("Colee") || nameFour.equals("colee"))
		  		{
		  			System.out.println("\nGo back to your desk and try writing some of your own code.");
		  			break;
		  		}
		  		else if(nameFour.equals("Kellen") || nameFour.equals("kellen"))
		  		{
		  			System.out.println("\nWelcome Sensei.");
		  			break;
		  		}
		  		else if(nameFour.equals("Mason") || nameFour.equals("mason"))
		  		{
		  			System.out.println("\nMasonnnnnnnnnnnnnn");
		  			break;
		  		}
		  		else
		  		{
		  			System.out.println("name not found in our databases. You are on the help queue.");
		  			break;
		  		}
  			}
  		}
  		return nameFour;
	}
	public static void printQueue()
	{
		System.out.println("________________________________________");
		System.out.println("             Help Queue:              ");
		System.out.println(" 1. " + name);
		System.out.println(" 2. " + nameOne);
		System.out.println(" 3. " + nameTwo);
		System.out.println(" 4. " + nameThree);
		System.out.println(" 5. " + nameFour);
		System.out.println("_________________________________________");
	}
}
