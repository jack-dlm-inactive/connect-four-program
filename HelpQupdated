import java.util.Scanner;
class Main 
{
	static Scanner sc = new Scanner(System.in);
	static String name = "";
	static String nameOne = "";
	static String nameTwo = "";
	static String nameThree = "";
	static String nameFour = "";
	static String nameFive = "";
	static String nameSix = "";
	static String nameSeven = "";
	static String nameEight = "";
	static String nameNine = "";
	
	public static void main(String[] args) 
	{
		getName();
		printQueue();
		getNameOne();
		printQueue();
		getNameTwo();
		printQueue();
		getNameThree();
		printQueue();
		getNameFour();
		printQueue();
		getNameFive();
		printQueue();
	}
	public static String getName()
	{
  		System.out.println("> state your name:");
  		name = sc.nextLine();
  		while(true)
  		{
	  		if(name.equals("Colee") || name.equals("colee"))
	  		{
	  			System.out.println("\nGo back to your desk and try writing some of your own code.");
	  			break;
	  		}
	  		else if(name.equals("Kellen") || name.equals("kellen"))
	  		{
	  			System.out.println("\nWelcome Sensei.");
	  			break;
	  		}
	  		else if(name.equals("Mason") || name.equals("mason"))
	  		{
	  			System.out.println("\nMasonnnnnnnnnnnnnn");
	  			break;
	  		}
	  		else
	  		{
	  			System.out.println("name not found in our databases. You are on the help queue.");
	  			break;
	  		}
  		}
  		return name;
	}
	public static String getNameOne()
	{
  		System.out.println("\nadd a second name to the help queue:");
  		nameOne = sc.nextLine();
  		while(true)
  		{
  			if(nameOne.equals(name))
  			{
  				System.out.println("your name is already on the queue!");
  				System.exit(0);
  			}
  			else
  			{
		  		if(nameOne.equals("Colee") || nameOne.equals("colee"))
		  		{
		  			System.out.println("\nGo back to your desk and try writing some of your own code.");
		  			break;
		  		}
		  		else if(nameOne.equals("Kellen") || nameOne.equals("kellen"))
		  		{
		  			System.out.println("\nWelcome Sensei.");
		  			break;
		  		}
		  		else if(nameOne.equals("Mason") || nameOne.equals("mason"))
		  		{
		  			System.out.println("\nMasonnnnnnnnnnnnnn");
		  			break;
		  		}
		  		else
		  		{
		  			System.out.println("name not found in our databases. You are on the help queue.");
		  			break;
		  		}
  			}
  		}
  		return nameOne;
	}
